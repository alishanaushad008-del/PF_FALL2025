#include<stdio.h>
int main()
{
    int n;
    int factorial;
     printf("Enter a number :");
     scanf("%d",&n);
     if (n == 0 || n == 1) {
        printf("Factorial does not exist!");
     }
        else{
     factorial=factorial*(n-1);
     printf("The factorial of %d is %d",n,factorial);

}
     }